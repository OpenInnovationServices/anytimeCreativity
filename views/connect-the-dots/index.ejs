            <script>
                $('#submitBtn').click(function(e) {
                    e.preventDefault();
                    var user = $("input.teamPlayerName").map(function() {
                        return this.value;
                    }).get().join(", <br>");

                    var ideaName = document.getElementById("ideaName").value;
                    var idea = document.getElementById("idea").value;
                    // var group3 = document.querySelector("#group3 > li > div > img").src;
                    // var group2 = document.querySelector("#group2 > li > div > img").src;
                    // var group2_text = document.querySelector("#group2 > li > div > p").innerText;
                    // var group3_text = document.querySelector("#group3 > li > div > p").innerText;

                    var user1 = document.getElementById("userName").value;
                    var group2 = $('#group2 li').attr('value');
                    var group3 = $('#group3 li').attr('value');

                    var group2_text = $('#group2 li').attr('data-image-name');
                    var group3_text = $('#group3 li').attr('data-image-name');

                    if (user && ideaName && idea && group2 && group3) {
                        $.ajax({
                            type: 'POST',
                            url: '/api/challenge/',
                            data: {
                                mix: group2,
                                match: group3,
                                mix_text: group2_text,
                                match_text: group3_text,
                                idea: document.getElementById("idea").value,
                                penName: user,
                                ideaName: document.getElementById("ideaName").value,
                                code: "1001",
                            },
                            success: function(data) {
                                $('#successMessage').show();
                                $('#submitBtn').prop("disabled", true);
                            },
                            error: function(data) {
                                $('#failedMessage').show();
                                console.log('An error occurred.');
                                console.log(data);
                            },
                        });
                    } else {
                        alert("Please fill all the values to submit");
                    }
                });
                var count = 1;
                $('.add-more').click(function() {
                    if (count >= 10) {
                        alert('Sorry, you can add only 10 players.');
                        return false;
                    }
                    var newRow = '<div class="form-group row text-left">' +
                        '<label for="playerName" class="col-sm-4 custom-label">Player\'s Name</label>' +
                        '<div class="col-sm-8">' +
                        '<input type="text" class="form-control teamPlayerName" id="playerName" maxlength="100" placeholder="Click + to enter each player">' +
                        '</div>' +
                        '</div>';
                    $('#ideaDiv').append(newRow);
                    count++;
                });
            </script>


